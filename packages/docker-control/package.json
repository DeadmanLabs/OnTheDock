{
  "name": "@org/docker-control",
  "version": "1.0.0",
  "description": "TypeScript library for Docker container management with realtime capabilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "docker",
    "container",
    "management",
    "typescript",
    "realtime"
  ],
  "dependencies": {
    "axios": "^1.6.2",
    "debug": "^4.3.4",
    "dockerode": "^4.0.0",
    "events": "^3.3.0",
    "p-retry": "^6.2.1",
    "stream": "^0.0.2",
    "tar-stream": "^3.1.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/dockerode": "^3.3.23",
    "@types/jest": "^29.5.10",
    "@types/tar-stream": "^3.1.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  }
}
